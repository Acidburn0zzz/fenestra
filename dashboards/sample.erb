<% content_for :title do %>My super sweet dashboard<% end %>
<div class="gridster" ng-controller="EventCtrl">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="2">
      <div ng-controller="ContributorsCtrl">
        <outside-contributors>
        </outside-contributors>
      </div>
    </li>

    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div ng-controller="BuzzwordsCtrl">
        <buzzword-list></buzzword-list>
      </div>
    </li>

    <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
      <div ng-controller="TweetCtrl">
        <comment-carousel comments="comments">
        </comment-carousel>
      </div>
      <i class="icon-twitter icon-background"></i>
    </li>

  </ul>
</div>
